        <b:includable id='main'>

  <div class='widget-content-popular-posts rounded shadow overflow-hidden mb-6 m-0 p-0'>
<b:if cond='data:title'><h2 class='text-sm bg-grey-lighter text-grey-darker py-3 px-4 border-b'><data:title/></h2></b:if>
    <ul class='list-reset p-1 m-0'>
      <b:loop values='data:posts' var='post'>
      <li class='m-0 p-1'>
        <b:if cond='data:showThumbnails == &quot;false&quot;'>
          <b:if cond='data:showSnippets == &quot;false&quot;'>
            <!-- (1) No snippet/thumbnail -->
            <a expr:href='data:post.href'><data:post.title/></a>
          <b:else/>
            <!-- (2) Show only snippets -->
            <div class='item-title'><a class='title h3' expr:href='data:post.href' expr:title='data:post.title'><data:post.title/></a></div>
            <div class='flex-auto item-snippet'><data:post.snippet/></div>
          </b:if>
        <b:else/>
          <b:if cond='data:showSnippets == &quot;false&quot;'>
            <!-- (3) Show only thumbnails -->
            <div class='item-thumbnail-only m0 p0 flex'>
              <b:if cond='data:post.thumbnail'>
                <div class='item-thumbnail flex-none mt1 ml1 p0'>
                  <a expr:href='data:post.href' target='_blank'>
                    <amp-img expr:alt='data:post.title' expr:height='data:thumbnailSize' expr:src='data:post.thumbnail' expr:title='data:post.title' expr:width='data:thumbnailSize'/>
                  </a>
                </div>
              </b:if>
              <div class='item-title m1 p0 flex-auto'><a expr:href='data:post.href' expr:title='data:post.title'><data:post.title/></a></div>
            </div>
            <div class='clear'/>
          <b:else/>
            <!-- (4) Show snippets and thumbnails -->
            <div class='item-content p-2'>
              <b:if cond='data:post.thumbnail'>
                <div class='item-thumbnail left flex-none'>
                  <a expr:href='data:post.href' target='_blank'>
